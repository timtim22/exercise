<h1>Tickets List</h1>

<table>
  <thead>
    <tr>
      <th>Request Number</th>
      <th>Sequence Number</th>
      <th>Request Type</th>
      <th>Request Action</th>
      <th>Date Times</th>
      <th>Service Area</th>
      <th>Digsite Info</th>
      <th>Show Details</th>
    </tr>
  </thead>
  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.request_number %></td>
        <td><%= ticket.sequence_number %></td>
        <td><%= ticket.request_type %></td>
        <td><%= ticket.request_action %></td>
        <td><%= ticket.date_times %></td>
        <td><%= ticket.service_area %></td>
        <td><%= ticket.digsite_info %></td>
        <td><%= link_to 'Show', ticket_path(ticket) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
